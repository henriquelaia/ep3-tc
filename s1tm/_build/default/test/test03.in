M:
  # TM deciding { w in {0,1}* | w ends with 0 }
  states: [s, q0, q1, qA, qR]
  input_alphabet: [0, 1]
  tape_alphabet_extra: [_]
  start_state: s
  accept_state: qA
  reject_state: qR

  delta:
    s:
      0: [q0, 0, R]
      1: [s, 1, R]
      _: [qR, _, R] # Empty string is rejected
    q0:
      0: [q0, 0, R]
      1: [s, 1, R]
      _: [qA, _, S] # Ends with 0 -> Accept

w: "1010"

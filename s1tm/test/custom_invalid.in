M:
  states: [s, qA, qR]
  input_alphabet: [0, 1]
  tape_alphabet_extra: [_, 0]  # INVALID: 0 repeated in input and extra
  start_state: s
  accept_state: qA
  reject_state: qR
  delta:
    s:
      _: [qA, _, S]
w: "0"

M:
  states: [s, qA]
  input_alphabet: [0, 1]
  tape_alphabet_extra: [_]
  start_state: s
  accept_state: qA
  reject_state: qA  # INVALID: accept == reject
  delta:
    s:
      _: [qA, _, S]
w: ""

M:
  states: [s, qA, qR]
  input_alphabet: [0, 1]
  tape_alphabet_extra: [_]
  start_state: s
  accept_state: qA
  reject_state: qR
  delta:
    s:
      0: [qZ, 0, R] # INVALID: qZ does not exist
w: "0"

M:
  states: [s, qA, qR]
  input_alphabet: [0]
  tape_alphabet_extra: [_]
  start_state: s
  accept_state: qA
  reject_state: qR
  delta:
    s:
      0: [qA, X, R] # INVALID: X not in alphabet
w: "0"
